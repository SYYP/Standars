<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical">
    <include layout="@layout/toolbar" android:id="@+id/lasdiueue"/>

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/base70dp"
            android:background="#c912b7f5"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tv_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base13dp"
                android:text="单位编码:"
                android:textColor="#101010"
                android:textSize="@dimen/base13dp" />

            <EditText
                android:editable="false"
                android:id="@+id/tv_edit_name"
                style="@style/edit"
                android:layout_width="@dimen/base85dp"
                android:layout_height="@dimen/base30dp"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base9dp"
                android:gravity="center"
                android:hint="请输入名称"
                android:textColor="#4c8fd2"
                android:textSize="@dimen/base13dp" />

            <TextView
                android:id="@+id/tv_type"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base13dp"
                android:text="构筑物编码:"
                android:textColor="#101010"
                android:textSize="@dimen/base13dp" />

            <EditText
                android:editable="false"
                android:id="@+id/tv_bm"
                style="@style/edit"
                android:layout_width="@dimen/base80dp"
                android:layout_height="@dimen/base30dp"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base9dp"
                android:drawableRight="@drawable/xiaxia"
                android:gravity="center"
                android:hint="请输入"
                android:textColor="#4c8fd2"
                android:textSize="@dimen/base13dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base13dp"
                android:text="@string/zb"
                android:textColor="#101010"
                android:textSize="@dimen/base13dp" />

            <EditText
                android:editable="false"
                android:id="@+id/tv_zb"
                style="@style/edit"
                android:layout_width="@dimen/base80dp"
                android:layout_height="@dimen/base30dp"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base9dp"
                android:drawableRight="@drawable/xiaxia"
                android:gravity="center"
                android:hint="请输入"
                android:textColor="#4c8fd2"
                android:textSize="@dimen/base13dp" />

            <TextView
                android:id="@+id/tv_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base13dp"
                android:text="时间:"
                android:textColor="#101010"
                android:textSize="@dimen/base13dp" />

            <LinearLayout
                android:layout_width="@dimen/base100dp"
                android:layout_height="@dimen/base30dp"
                android:layout_gravity="center"
                android:background="@drawable/editext_shape_two">

                <EditText
                    android:drawablePadding="5dp"
                    android:drawableRight="@drawable/calendar"
                    android:id="@+id/ed_muchtrend_strattime"
                    android:layout_width="@dimen/base90dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@null"
                    android:editable="false"
                    android:textColor="#4c8fd2"
                    android:textSize="@dimen/base15dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/tv_time2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base13dp"
                android:text="至"
                android:textColor="#101010"
                android:textSize="@dimen/base13dp" />

            <LinearLayout
                android:layout_width="@dimen/base100dp"
                android:layout_height="@dimen/base30dp"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base10dp"
                android:background="@drawable/editext_shape_two">

                <EditText
                    android:padding="5dp"
                    android:drawableRight="@drawable/calendar"
                    android:id="@+id/ed_muchtrend_endtime"
                    android:layout_width="@dimen/base90dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@null"
                    android:editable="false"
                    android:textColor="#4c8fd2"
                    android:textSize="@dimen/base15dp" />
            </LinearLayout>

            <TextView
                android:id="@+id/tv_query"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="@dimen/base13dp"
                android:layout_marginRight="@dimen/base10dp"
                android:text="查询"
                android:textColor="#4c8fd2"
                android:textSize="@dimen/base13dp" />
        </LinearLayout>
    </HorizontalScrollView>

    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/contentView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </LinearLayout>
</LinearLayout>
